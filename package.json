  "scripts": {
    "clean": "rm -rf node_modules && rm -rf .next && rm -rf .contentlayer",
    "dev": "concurrently \"contentlayer dev\" \"next dev\" \"turso dev --db-file drizzle/db/local.db\"",
    "build": "contentlayer build && next build",
    "turbo": "next dev --turbo",
    "start": "next start",
    "lint": "next lint",
    "preview": "next build && next start",
    "db:generate": "dotenv drizzle-kit generate:sqlite",
    "db:migrate": "dotenv -c -- tsx ./src/lib/db/dbmigrate.ts",
    "db:push": "dotenv drizzle-kit push:sqlite",
    "db:introspect": "dotenv drizzle-kit introspect:sqlite",
    "db:seed": "dotenv tsx drizzle/db/seed.ts",
    "db:studio": "dotenv drizzle-kit studio --verbose"
  },
